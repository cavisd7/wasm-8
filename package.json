{
  "name": "chip8",
  "version": "1.0.0",
  "description": "CHIP-8 emulator made with AssemblyScript",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:asc && webpack",
    "build:asc": "asc --optimize -b ./core/wasm8.wasm -t ./core/wasm8.wat -d ./core/types.d.ts ./core/assemblyscript/index.ts",
    "start": "npm run build:asc && webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cavisd7/wasm-8.git"
  },
  "author": "Casey Davis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cavisd7/wasm-8/issues"
  },
  "homepage": "https://github.com/cavisd7/wasm-8#readme",
  "dependencies": {
    "@assemblyscript/loader": "^0.17.3"
  },
  "devDependencies": {
    "assemblyscript": "0.17.2",
    "ts-loader": "8.0.11",
    "typescript": "4.1.2",
    "wasm-loader": "1.3.0",
    "webpack": "5.6.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0"
  }
}
